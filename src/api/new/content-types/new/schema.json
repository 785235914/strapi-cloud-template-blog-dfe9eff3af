{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "new",
    "pluralName": "news",
    "displayName": "New"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "subtitle": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "view_count": {
      "type": "integer",
      "required": false,
      "default": 0,
      "unique": false,
      "min": 0
    },
    "Date": {
      "type": "datetime"
    },
    "content": {
      "type": "blocks"
    },
    "markdownText": {
      "type": "richtext"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "contentMedia": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "cloudinaryMedia": {
      "type": "customField",
      "customField": "plugin::cloudinary-media-library.cloudinary"
    },
    "modules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::module.module",
      "mappedBy": "news"
    }
  }
}
