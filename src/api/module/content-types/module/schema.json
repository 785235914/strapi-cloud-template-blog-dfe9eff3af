{
  "kind": "collectionType",
  "collectionName": "modules",
  "info": {
    "singularName": "module",
    "pluralName": "modules",
    "displayName": "Module"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "subTitle": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "contentMedia": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "cloudinaryMedia": {
      "type": "customField",
      "customField": "plugin::cloudinary-media-library.cloudinary"
    },
    "content": {
      "type": "blocks"
    },
    "markdownText": {
      "type": "richtext"
    },
    "news": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::new.new",
      "inversedBy": "modules"
    }
  }
}
